{
  "plugins": ["prettier"],
  "extends": ["plugin:prettier/recommended", "eslint-config-atomic"],
  "rules": {
    "@typescript-eslint/quotes": ["error", "double"],
    "require-await": "off"
  },
  "ignorePatterns": [
    "node_modules/",
    "build/",
    "lib/",
    "tmp/",
    "v5-compat.d.ts",
    "draft.d.ts",
    "script/*.js",
    "script/*.d.ts"
  ]
}

{
  "mochaExplorer.parallel": true,
  "mochaExplorer.globImplementation": "vscode",
  "mochaExplorer.nodeArgv": ["--expose-gc"],
  "mochaExplorer.debuggerConfig": "JS-Attach",
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.cache": true,
    "**/script/*.js": true,
    "**/script/*.mjs": true,
    "**/script/*.js.map": true,
    "**/script/*.mjs.map": true,
    "**/script/*.d.ts": true,
    "**/script/*.d.mts": true,
    "**/script/*.tsbuildinfo": true
  }
}
